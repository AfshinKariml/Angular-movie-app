{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "node build.js && ng build --configuration production",
  "outputDirectory": "dist/movie-app",
  "installCommand": "npm install",
  "framework": "angular",
  "env": {
    "NODE_ENV": "production"
  }
}